    "format": "prettier --write .",
    "pre-check": "run-s format lint:fix lint",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.1",
