{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --ext .ts,.tsx",
    "lint:types": "tsc --noEmit",
    "lint:secret": "secretlint --maskSecrets --secretlintignore .gitignore '**/*'",
